import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter an input: ");
        int input = sc.nextInt();
        int geoSeq = 1;
        while(input>=0){
            geoSeq *= 2;
            input--;
            System.out.print(geoSeq + ", ");
         }
    }
}
